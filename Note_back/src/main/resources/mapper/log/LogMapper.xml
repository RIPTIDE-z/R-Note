<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riptidez.notebackend.mapper.log.LogMapper">

    <!-- 如果 id 是自增，用 useGeneratedKeys 回填 -->
    <insert id="insert"
            parameterType="com.riptidez.notebackend.entity.log.Log"
            useGeneratedKeys="true"
            keyProperty="id">
        INSERT INTO student_operation_log
            (student_id, operation_time)
        VALUES
            (#{studentId}, #{operationTime})
    </insert>

</mapper>
